#, c-format
msgid ""
"Finance statistics:\n"
"Profit:  %8.2f\n"
"Costs:   %8.2f\n"
"Balance: %8.2f\n"
msgstr ""
"Отчет:\n"
"Доход:   %8.2f\n"
"Расход:  %8.2f\n"
"Остаток: %8.2f\n"
