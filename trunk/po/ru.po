msgid "String is too small"
msgstr "Слишком короткая строка"

msgid "First field of string should be sign '+' or '-'!"
msgstr "Первым полем в строке должен быть знак '+' или '-'!"

msgid "Separator for fields should be sign '|'!"
msgstr "Разделителем для полей должен быть знак '|'!"

msgid "Separator after 3rd field not found!"
msgstr "Не найден разделитель после 3-его поля!"

msgid "Separator after 4th field not found!"
msgstr "Не найден разделитель после 4-ого поля!"

msgid "Date should consist of digitals only!"
msgstr "Дата должна состоять только из чисел!"

msgid "Separator for date should be sign '.'!"
msgstr "Разделителем для даты является знак '.'!"

msgid "Invalid number of day"
msgstr "Неправильный день месяца"

msgid "Invalid number of month"
msgstr "Неправильный номер месяца"

msgid "Invalid number of year! Year should be more then 0"
msgstr "Неправильный год! Год должен быть больше нуля"

msgid "Invalid day of month in leap year"
msgstr "Неправильный день месяца в високосном году"

msgid "Invalid day of month"
msgstr "Неправильный день месяца"

msgid "error occurs"
msgstr "произошла ошибка"

msgid "Date in future"
msgstr "Дата в будущем"

#, c-format
msgid "-> Trying to get statistics for %s file\n"
msgstr "-> Получение информации для файла %s\n"

#, c-format
msgid "File %s isn't regular file!\n"
msgstr "Файл %s не является обычным файлом!\n"

msgid "Open data file"
msgstr "Открываю файл с данными"

msgid "Lock file"
msgstr "Блокирую файл"

msgid "cannot lock file for writing"
msgstr "не удалось заблокировать файл для записи"

msgid "Writing data"
msgstr "Записываю данные"

msgid "Unlock file"
msgstr "Разблокирование файла"

msgid "cannot unlock file"
msgstr "не удалось разблокировать файл"

msgid "Flushing data to disk"
msgstr "Сбрасываю данные на диск"

msgid "Close file"
msgstr "Закрываю файл"

#, c-format
msgid ""
"%s: Your private financial manager\n"
"\n"
"Usage: %s [option]\n"
"  -v\tenable verbose mode\n"
"  -V\tprint version and exit\n"
"  -h\tprint this help and exit\n"
msgstr ""
"%s: Ваш личный финансовый менеджер\n"
"\n"
"Использование: %s [опция]\n"
"  -v\tвключить режим детализации действий\n"
"  -V\tвывести версию програмы и выйти\n"
"  -h\tвывести эту помощь и выйти\n"

msgid "version"
msgstr "версия"

msgid "return"
msgstr "вернула"

msgid "NOTE: Set verbose level to"
msgstr "Внимание: Уровень режима детализации действий установлен в"

msgid "Using default data file..."
msgstr "Используется файл с данными по умолчанию..."

msgid "cannot allocate memory"
msgstr "не удалось выделить память"

msgid "Not enough arguments for action"
msgstr "Недостаточно аргументов для действия"

msgid "Wrong argument for action"
msgstr "Неверный аргумент для действия"

msgid "cannot get value for $HOME variable"
msgstr "не удалось получить значение переменной $HOME"

msgid "cannot find user with uid"
msgstr "не удалось найти пользователя с идентификатором"

msgid "Your home directory is"
msgstr "Ваш домашний каталог"

#, c-format
msgid ""
"Writing data was truncated.\n"
"Please notify the author about this incident!\n"
msgstr ""
"Во время записи данные были сокращены.\n"
"Пожалуйста сообщите автору об этом инциденте!\n"

#, c-format
msgid "Failed to write to the character string\n"
msgstr "Не удалось записать в символьную строку\n"

msgid "Failed to open file"
msgstr "Ошибка при открытии файла"

msgid "Reading data..."
msgstr "Чтение данных..."

#, c-format
msgid "Too many wrong lines in database. Exit.\n"
msgstr "Слишком много некорректных записей в базе. Выхожу.\n"

#, c-format
msgid "-> Reads %lu strings"
msgstr "-> Прочитано %lu строк"

#, c-format
msgid " and %lu records"
msgstr " и %lu записей"

msgid "from data file"
msgstr "из файла"

#, c-format
msgid ""
"Finance statistics:\n"
"Profit:  %8.2f\n"
"Costs:   %8.2f\n"
"Balance: %8.2f\n"
msgstr ""
"Отчет:\n"
"Доход:   %8.2f\n"
"Расход:  %8.2f\n"
"Остаток: %8.2f\n"
